{
	"name": "npm-link-cli",
	"version": "0.1.0",
	"description": "Get a link to a given npm package.",
	"keywords": [
		"cli",
		"cli-app",
		"command-line",
		"package",
		"npm",
		"github",
		"gh",
		"repo",
		"repository",
		"cwd",
		"link",
		"clipboard"
	],
	"license": "MIT",
	"repository": "tommy-mitchell/npm-link-cli",
	"author": {
		"name": "Tommy Mitchell",
		"url": "https://tommymitchell.io"
	},
	"type": "module",
	"bin": {
		"npm-link": "dist/cli.js"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": "^18.18 || ^20.8 || ^21"
	},
	"scripts": {
		"prepare": "npm run build",
		"build": "tsc -p tsconfig.build.json && execify --all",
		"lint": "xo",
		"format": "dprint fmt && xo --fix",
		"test": "tsc --noEmit && c8 ava"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsimp",
			"--import=esmock"
		],
		"environmentVariables": {
			"concurrency": "10"
		}
	},
	"dependencies": {
		"clipboardy": "^4.0.0",
		"github-url-from-git": "^1.5.0",
		"is-url-superb": "^6.1.0",
		"log-symbols": "^6.0.0",
		"meow": "^13.2.0",
		"package-json": "^10.0.0",
		"read-package-up": "^11.0.0",
		"terminal-link": "^3.0.0"
	},
	"devDependencies": {
		"@shopify/semaphore": "^3.1.0",
		"@tommy-mitchell/dprint-config": "^0.1.1",
		"@tommy-mitchell/eslint-config-xo": "^0.3.1",
		"@tommy-mitchell/tsconfig": "^2.1.0",
		"@types/github-url-from-git": "^1.5.3",
		"@types/node": "^18.18",
		"ava": "^6.1.3",
		"c8": "^10.1.2",
		"esmock": "^2.6.6",
		"execa": "^9.3.0",
		"execify-cli": "beta",
		"get-executable-bin-path": "^0.1.0",
		"has-ansi": "^6.0.0",
		"strip-ansi": "^7.1.0",
		"tsimp": "^2.0.11",
		"type-fest": "^4.21.0",
		"typescript": "~5.5.3",
		"xo": "^0.58.0"
	}
}
